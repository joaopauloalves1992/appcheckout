{
  "version": 2,
  "builds": [
    {
      "src": "backend/checkout_backend/src/main.py",
      "use": "@vercel/python"
    },
	{
      "src": "backend_src/main.py",
      "use": "@vercel/python"
    },
	{
      "src": "frontend/checkout_frontend/package.json",
      "use": "@vercel/node",
      "config": { "includeFiles": ["frontend/**"] }
    }
  ],
  "routes": [
    {
      "src": "backend/checkout_backend/",
      "dest": "src/main.py"
    },
	{
      "src": "backend_src/",
      "dest": "/main.py"
    },
    {
      "src": "backend_src/static/",
      "dest": "/index.html"
    },
	{
      "src": "backend/checkout_backend/src/static",
      "dest": "/index.html"
    }
  ]
}
